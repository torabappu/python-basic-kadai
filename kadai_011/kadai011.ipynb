      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "#for文\n",
        "for _ in range(2):\n",
        "  for i in array:\n",
        "   print (i)\n",
        "\n",
        "#while文\n",
        "repeat_count = 0\n",
        "\n",
        "while repeat_count < 2:\n",
        "    index = 0\n",
        "\n",
        "    while index < len(array):\n",
        "        print(array[index])\n",
        "        index += 1 \n",
        "\n",
        "    repeat_count += 1"
      ]
